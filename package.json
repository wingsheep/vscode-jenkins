{
  "publisher": "wingsheep",
  "name": "easy-build-jenkins",
  "displayName": "Easy Build Jenkins Job",
  "version": "0.0.2",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "Fast and convenient way to build Jenkins jobs",
  "author": "Wingsheep <1306750238@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wingsheep",
  "homepage": "https://github.com/wingsheep/vscode-jenkins#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/wingsheep/vscode-jenkins"
  },
  "bugs": {
    "url": "https://github.com/wingsheep/vscode-jenkins/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jenkins - Configuration",
      "properties": {
        "jenkins.domain": {
          "type": "string",
          "description": "Custom Jenkins domain",
          "default": ""
        },
        "jenkins.userName": {
          "type": "string",
          "description": "UserName of the Jenkins",
          "default": ""
        },
        "jenkins.passwordKey": {
          "type": "string",
          "description": "The key for the Jenkins password in system variables",
          "default": "JENKINS_PASSWORD"
        },
        "jenkins.jobName": {
          "type": "string",
          "description": "JobName of the Jenkins",
          "default": ""
        },
        "jenkins.params": {
          "type": "object",
          "description": "Params of the Jenkins",
          "default": {}
        },
        "jenkins.isBuildCurrentBranch": {
          "type": "boolean",
          "description": "Whether to build the current branch",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "jenkins.buildJob",
        "title": "Jenkins: Build job"
      },
      {
        "command": "jenkins.killJob",
        "title": "Jenkins: Stop build job"
      },
      {
        "command": "jenkins.generateSetting",
        "title": "Jenkins: Generate setting"
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "publish": "vsce publish",
    "pack": "vsce package",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish",
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "dependencies": {
    "absolute": "^0.0.1",
    "node-fetch": "^2",
    "simple-git": "^3.21.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/absolute": "^0.0.31",
    "@types/jenkins": "^1.0.2",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.9",
    "@types/vscode": "^1.83.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "pnpm": "^8.10.5",
    "rimraf": "^5.0.5",
    "ts-loader": "^9.5.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
